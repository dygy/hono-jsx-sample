{
  "type": "module",
  "name": "hono-jsx-sample",
  "author": "dygy",
  "scripts": {
    "dev": "concurrently \"vite\" \"tailwindcss -i ./src/root.css -o ./public/static/style.css --minify --watch\"",
    "build": "tailwindcss -i ./src/root.css -o ./public/static/style.css --minify && vite build && vite build --mode client",
    "start": "wrangler pages dev dist",
    "binome": "npx @biomejs/biome format --write ./src"
  },
  "dependencies": {
    "hono": "4.2.9"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.2",
    "@cloudflare/workers-types": "4.20240423.0",
    "@hono/vite-cloudflare-pages": "0.2.5",
    "@hono/vite-dev-server": "0.3.5",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "cssnano": "6.1.2",
    "postcss": "8.4.38",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "3.4.3",
    "vite": "5.2.10",
    "vite-tsconfig-paths": "4.3.2",
    "wrangler": "3.52.0"
  }
}
