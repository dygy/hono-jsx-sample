{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"lib": ["esnext"],
		"types": ["@cloudflare/workers-types", "vite/client"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": "./app",
		"paths": {
			"@islands/*": ["islands/*"],
			"@layouts/*": ["layouts/*"],
			"@pages/*": ["pages/*"],
			"@routes/*": ["routes/*"],
			"@utils/*": ["utils/*"]
		}
	}
}
